    "build":   "curl -s https://raw.githubusercontent.com/geolonia/tilebuilder-docker/refs/heads/main/docker-compose.yml | docker-compose -f - run --rm tilebuilder",
    "start":   "curl -s https://raw.githubusercontent.com/geolonia/tilebuilder-docker/refs/heads/main/docker-compose.yml | docker-compose -f - run --rm tileserver"
